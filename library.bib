issn = {09203796},
journal = {Fusion Engineering and Design},
keywords = {Finite elements,Helium cooled pebble bed,Test blanket module},
pages = {607--612},
title = {{Design update, thermal and fluid dynamic analyses of the EU-HCPB TBM in vertical arrangement}},
volume = {84},
year = {2009}
}

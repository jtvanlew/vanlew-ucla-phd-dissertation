isbn = {0022-3131},
issn = {0022-3131},
journal = {Journal of Nuclear Science and Technology},
keywords = {fusion demo reactor,nuclear analysis,optimal control,solid breeder blankets,supercritical water,thermal anal-,tritium breeding ratio,ysis},
number = {11},
pages = {1014--1018},
title = {{Nuclear and Thermal Analyses of Supercritical-water-cooled Solid Breeder Blanket for Fusion DEMO Reactor}},

@article{Yanagi2001,
abstract = {Within a design study of a fusion DEMO reactor aiming at demonstrating technologies of fusion power plant. supercritical water is applied as a coolant of solid breeder blanket to attain high thermal efficiency. The blanket has multi-layer composed of solid breeder pebbles (Li2O) and neutron multiplier pebbles (Be) which are radially separated by cooling panels. The first wall and the breeding region are cooled by supercritical water below and above the pseudo-critical temperature, respectively. Temperature distribution and tritium breeding ratio (TBR) have been estimated by one-dimensional nuclear and thermal calculation,,. The local TBR as high as 1.47 ha, been obtained After optimization of temperature distribution in the breeder region under the following conditions: neutron wall loading of 5 MW/m(2), Li-6 enrichment of 30{\%} and coolant temperature at inlet of breeder region of 380degreesC. In the ease of the higher coolant temperature 430degreesC of the breeder region the local TBR was reduced to be 1.40. This means that the net TBR higher than 1.0 could be expected with the supercritical-water-cooled blanket, whose temperature distribution in the breeder region would be optimized by following the coolant temperature, and where a coverage of the breeder region is assumed to be 70{\%}.},
author = {Yanagi, Yoshihiko and Sato, Satoshi and Enoeda, Mikio and Hatano, Toshihisa and Kikuchi, Shigeto and Kuroda, Toshimasa and Kosaku, Yasuo and Ohara, Yoshihiro},

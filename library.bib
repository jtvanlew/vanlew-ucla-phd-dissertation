issn = {09203796},
journal = {Fusion Engineering and Design},
keywords = {pebble beds,solid breeder blanket,thermomechanical performance},
pages = {605--611},
title = {{Numerical and experimental prediction of the thermomechanical performance of pebble beds for solid breeder blanket}},
volume = {49-50},
year = {2000}
}
@article{Zeiser2002,
abstract = {The pressure drop of technical devices is a crucial property for their design and operation. In this paper, we show how the results of lattice Boltzmann simulations can be used in science and engineering to improve the physical understanding of the pressure drop and the flow inhomogeneities in porous media, especially in sphere-packed fixed-bed reactors with low aspect ratios. Commonly used pressure drop correlations are based on simplified assumptions such as the capillary or tortuosity model, which do not reflect all hydrodynamic effects. Consequently, empirical correlations for certain classes of media have been introduced in the past to bridge the gap between the models and the experimental findings. As is shown in this paper by the detailed analysis of the velocity field in the void space of packed beds, the pressure drop is due to more complex hydrodynamics than considered in the above-mentioned models. With the help of lattice Boltzmann simulations, we were able to analyse the different contributions to the total dissipation, namely shear and deformation of the fluid, for different geometries over a wide range of Reynolds numbers. We further show that the actual length of the flow paths changes considerably with the radial and circumferential position.},
author = {Zeiser, Thomas and Steven, Martin and Freund, Hannsj{\"{o}}rg and Lammers, Peter and Brenner, Gunther and Durst, Franz and Bernsdorf, J{\"{o}}rg},
doi = {10.1098/rsta.2001.0945},
file = {:F$\backslash$:/Documents/Mendeley Desktop/Zeiser et al/Philosophical transactions. Series A, Mathematical, physical, and engineering sciences/Zeiser et al. - 2002 - Analysis of the flow field and pressure drop in fixed-bed reactors with the help of lattice Boltz.pdf:pdf},
isbn = {1364-503X},
issn = {1364-503X},
journal = {Philosophical transactions. Series A, Mathematical, physical, and engineering sciences},
keywords = {lattice boltzmann metho d,pressur e drop,randomly packed b eds,shear and deform ation,xed-b ed reactors},
number = {1792},
pages = {507--520},
pmid = {16214691},
title = {{Analysis of the flow field and pressure drop in fixed-bed reactors with the help of lattice Boltzmann simulations.}},
volume = {360},
year = {2002}
}

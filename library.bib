file = {:F$\backslash$:/Documents/Mendeley Desktop/Narv{\'{a}}ez et al/Journal of Statistical Mechanics Theory and Experiment/Narv{\'{a}}ez et al. - 2013 - From creeping to inertial flow in porous media a lattice Boltzmann–finite element study.pdf:pdf},
issn = {1742-5468},
journal = {Journal of Statistical Mechanics: Theory and Experiment},
number = {02},
pages = {P02038},
title = {{From creeping to inertial flow in porous media: a lattice Boltzmann–finite element study}},

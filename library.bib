@article{Hunt1990,
abstract = {Traditional models of transport in packed beds assume that the velocity$\backslash$nacross the reactor is uniform, even along the solid surfaces. Because$\backslash$nof the velocity-slip assumption, corresponding convection studies$\backslash$nfor inert beds rely on a temperature-slip boundary condition to predict$\backslash$nexperimental data. For reacting beds, the transport process is more$\backslash$ncomplex; however, the analysis is simplified using the temperature-slip$\backslash$nheat transfer coefficients determined under non-reacting conditions.$\backslash$nSince the heat transfer rates differ between reacting and inert beds,$\backslash$nconsiderable discrepancies often exist between predicted results$\backslash$nand experimental measurements. Furthermore, using the boundary conditions$\backslash$nfrom inert beds precludes the determination of the actual wall heat$\backslash$ntransfer for catalytic reactions. The present non-Darcian analysis$\backslash$nassumes a porosity variation near the wall and solves the governing$\backslash$nmomentum equation satisfying that noslip boundary condition. The$\backslash$ncorresponding heat transfer solutions use the wall-temperature boundary$\backslash$ncondition and hence can predict cooling rates when chemical reactions$\backslash$nare included. The analysis yields results that agree with existing$\backslash$nexperimental data.},
author = {Hunt, M.L. and Tien, C.L.},
doi = {10.1016/0009-2509(90)87080-C},
file = {:F$\backslash$:/Documents/Mendeley Desktop/Hunt, Tien/Chemical Engineering Science/Hunt, Tien - 1990 - Non-darcian flow, heat and mass transfer in catalytic packed-bed reactors.pdf:pdf},

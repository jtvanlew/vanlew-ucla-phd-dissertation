@article{Parmigiani2011,
abstract = {Fluid flows in porous media are ubiquitous in the shallow Earth environments.$\backslash$nThe resistivity of the porous medium to the flow of either a single$\backslash$nor multiphase fluid does not only effect the fluid discharge trough$\backslash$nthe porous medium, but also the transport of additional dissolved$\backslash$ncomponents or heat. In many instances, chemicals or heat drive reactions$\backslash$nat the fluid-solid interface. These reactions can affect the porous$\backslash$nmedia geometry at the pore-scale leading to a local change in permeability$\backslash$nand phases saturation. Reactions can therefore have a drastic effect$\backslash$non discharge of mass and advected quantities. In this thesis we aim$\backslash$nto better understand and quantify the complex coupling between the$\backslash$ndifferent physical processes that take place at the pore-scale for$\backslash$nboth single and multiphase fluids in isothermal and thermal reactive$\backslash$nenvironments. We do that by performing pore-scale numerical simulations$\backslash$nfor both single and multiphase fluids in synthetic porous media samples.$\backslash$nThe numerical algorithm we choose is the lattice Boltzmann method.},
author = {Parmigiani, Andrea},
file = {:home/jon/Documents/Papers/Parmigiani - 2011 - Lattice Boltzmann calculations of reactive multiphase flows in porous media.pdf:pdf},
journal = {Cuiwww.Unige.Ch},
pages = {129},
title = {{Lattice Boltzmann calculations of reactive multiphase flows in porous media}},
url = {http://cuiwww.unige.ch/~chopard/CA/parmigiani.pdf},
year = {2011}
}

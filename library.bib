issn = {00223115},
journal = {Journal of Nuclear Materials},
keywords = {SIBELIUS},
mendeley-tags = {SIBELIUS},
pages = {168--172},
publisher = {Elsevier Science Publishers B.V.},
title = {{Compatibility behavior of beryllium with LiAlO2 and Li2ZrO3 ceramics, with 316 L and 1.4914 steels in SIBELIUS}},
url = {http://www.sciencedirect.com/science/article/pii/S0022311509800263},
volume = {191-194},
year = {1992}
}

issn = {00179310},
journal = {International Journal of Heat and Mass Transfer},
keywords = {Lattice Boltzmann method,Local thermal non-equilibrium,Natural convection,Porous media},
pages = {979--989},
publisher = {Elsevier Ltd},
title = {{A thermal lattice Boltzmann model for natural convection in porous media under local thermal non-equilibrium conditions}},
url = {http://dx.doi.org/10.1016/j.ijheatmasstransfer.2013.11.050},
volume = {70},

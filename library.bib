issn = {09203796},
journal = {Fusion Engineering and Design},
keywords = {blanket for fusion reactors,ceramic breeder,conductivity,experimental,li4sio4,lithiumorthosilicate,pebble beds,thermal conductivity measurement of},
mendeley-tags = {conductivity,experimental,li4sio4},
month = nov,
number = {1-4},

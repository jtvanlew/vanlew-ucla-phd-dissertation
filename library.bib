journal = {Nature Biotechnology},
keywords = {Adipose Tissue,Adipose Tissue: cytology,Animals,Bone Marrow Cells,Bone Marrow Cells: cytology,Lactic Acid,Mice,Polyglycolic Acid,Polymers,Skull,Skull: cytology,Stromal Cells,Stromal Cells: cytology},

journal = {Fusion Engineering and Design},
keywords = {DEMO relevancy,Finite elements,Helium-Cooled Pebble-Bed,Test Blanket Module},
number = {10-12},
pages = {2040--2044},
publisher = {Elsevier B.V.},
title = {{Preliminary thermal design and related DEMO relevancy of the EU-HCPB TBM in vertical arrangement}},
